{{#if comparison.wasCompared}}
  {{#if comparison.isDifferent}}
    {{comparison.pdiff.diffPercentage}}% different
    {{fa-icon "square"}}
  {{else}}
    {{fa-icon "square"}}
  {{/if}}
{{else}}
  {{#if comparison.wasAdded}}
    New snapshot
    {{fa-icon "plus-square"}}
  {{/if}}
  {{#if comparison.wasRemoved}}
    Removed snapshot
    {{fa-icon "minus-square"}}
  {{/if}}
{{/if}}
